{
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOW-FROM https://web.telegram.org" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://web.telegram.org" }
      ]
    }
  ]
} 